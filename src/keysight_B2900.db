record(aai, "$(P)$(R)FETCH_ARRAY") {
    field(DESC, "Fetch selected array after acquisition")
    field(DTYP, "stream")
    field(INP,  "@keysight_B2900.proto getArray(:FETC:ARR) $(PORT) $(A)")
    field(SCAN, "Passive")
    field(NELM, "$(FETCH_SIZE)")
    field(FTVL, "FLOAT")
    field(FLNK, "$(P)$(R)Array_Parser")
}

# -----------------------------------------------------------------------------

record(aSub, "$(P)$(R)Array_Parser") {
    field(DESC, "Parsing the output Array")
    field(SNAM, "arrayParse")
    field(INPA, "$(P)$(R)FETCH_ARRAY")
    field(FTA,  "FLOAT")
    field(NOA,  "$(FETCH_SIZE)")
    field(SCAN, "Passive")
    # outputs
    field(OUTA, "$(P)$(R)CURR_ARRAY PP")
    field(FTVA, "FLOAT")
    field(NOVA, "$(FETCH_SIZE)")
    field(OUTB, "$(P)$(R)TIME_ARRAY PP")
    field(FTVB, "FLOAT")
    field(NOVB, "$(FETCH_SIZE)")
}

record(aao, "$(P)$(R)CURR_ARRAY") {
    field(DESC, "Current array parsed after fetch")
    field(SCAN, "Passive")
    field(NELM, "$(DATA_SIZE)")
    field(FTVL, "FLOAT")
}

record(aao, "$(P)$(R)TIME_ARRAY") {
    field(DESC, "Time array parsed after fetch")
    field(SCAN, "Passive")
    field(NELM, "$(DATA_SIZE)")
    field(FTVL, "FLOAT")
}
