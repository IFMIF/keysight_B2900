
# Common IEEE-488.2 Commands
#
record(stringin, "$(P)$(R)IDN") {
    field(DESC, "SCPI identification string")
    field(DTYP, "stream")
    field(INP,  "@keysight_B2900.proto getIDN(39) $(PORT) $(A)")
    field(PINI, "YES")
}
record(waveform, "$(P)$(R)IDNwf") {
    field(DESC, "SCPI identification string ")
    field(DTYP, "stream")
    field(INP,  "@keysight_B2900.proto getIDN(199) $(PORT) $(A)")
    field(PINI, "YES")
    field(FTVL, "CHAR")
    field(NELM, "200")
}
record(ai, "$(P)$(R)OPC") {
    field(DESC, "Operation Complete bit")
    field(DTYP, "stream")
#    field(ZNAM, "OFF")
#    field(ONAM, "ON")
    field(SCAN, "Event")
    field(INP,  "@keysight_B2900.proto getD(*OPC) $(PORT) $(A)")
    field(FLNK, "$(P)$(R)FETCH_ARRAY")
    field(PINI, "YES")
}
