record(bo, "$(P)$(R)INIT_ACQ") {
    field(DESC, "Initiate Acquisition")
    field(DTYP, "stream")
    field(OUT,  "@keysight_B2900.proto cmd(:INIT:ACQ) $(PORT) $(A)")
    field(FLNK, "$(P)$(R)OPC")
}
