record(aSub, "$(P)$(R)Array_Parser") {
    field(DESC, "Parsing the output Array")
    field(SNAM, "arrayParse")
    field(INPA, "$(P)$(R)FETCH_ARRAY")
    field(FTA,  "FLOAT")
    field(NOA,  "$(FETCH_SIZE)")
    # outputs
    field(OUTA, "$(P)$(R)CURR_ARRAY")
    field(FTVA, "FLOAT")
    field(NOVA, "$(FETCH_SIZE)")
    field(OUTB, "$(P)$(R)TIME_ARRAY")
    field(FTVB, "FLOAT")
    field(NOVB, "$(FETCH_SIZE)")
}

record(aao, "$(P)$(R)CURR_ARRAY") {
    field(DESC, "Current array parsed after fetch")
    field(SCAN, "Passive")
    field(NELM, "$(FETCH_SIZE)")
    field(FTVL, "FLOAT")
}

record(aao, "$(P)$(R)TIME_ARRAY") {
    field(DESC, "Time array parsed after fetch")
    field(SCAN, "Passive")
    field(NELM, "$(FETCH_SIZE)")
    field(FTVL, "FLOAT")
}